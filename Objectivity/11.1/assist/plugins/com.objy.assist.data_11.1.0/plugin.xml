   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="com.objy.assist.core.model.dom.IDomComponent"
            nameFilter="Object"
            id="com.objy.assist.data.object">
         <action
               label="Delete"
               icon="icons/remove.gif"
               class="com.objy.assist.data.actions.DeleteObjectsActionDelegate"
               menubarPath="additions"
               enablesFor="+"
               id="com.objy.assist.data.action1">
            <enablement>
               <and>
	               <objectState
	                        name="preference.com.objy.assist.core.connectionMode"
	                        value="readWrite">
	               </objectState>
	               <or>
	                  <objectState
	                        name="parent.typeName"
	                        value="PqlQuery">
	                  </objectState>
	                  <objectState
	                        name="isReadOnly"
	                        value="false">
	                  </objectState>
	               </or>
               </and>
            </enablement>
         </action>
         <action
               label="Add Scope Name"
               class="com.objy.assist.data.actions.AddNameScopeActionDelegate"
               menubarPath="additions"
               enablesFor="+"
               id="com.objy.assist.data.addNameScope">
            <enablement>
               <and>
                  <objectState
	                       name="preference.com.objy.assist.core.connectionMode"
	                       value="readWrite">
	               </objectState>
                   <objectState
                           name="isReadOnly"
                           value="false">
                   </objectState>
               </and>
            </enablement>
         </action>
      </objectContribution>
      <objectContribution
            objectClass="com.objy.assist.core.model.dom.IDomComponent"
            nameFilter="ContainerData"
            id="com.objy.assist.data.container">
         <action
               label="Delete"
               icon="icons/remove.gif"
               class="com.objy.assist.data.actions.DeleteContainersActionDelegate"
               menubarPath="additions"
               enablesFor="+"
               id="com.objy.assist.data.action2">
            <enablement>
               <and>
                  <objectState
	                    name="preference.com.objy.assist.core.connectionMode"
	                    value="readWrite">
	              </objectState>
                  <objectState
                        name="isReadOnly"
                        value="false">
                  </objectState>
                  <not>
                     <objectState
                           name="class"
                           value="ooDefaultContObj">
                     </objectState>
                  </not>
               </and>
            </enablement>
         </action>
         <action
               label="Add Object(s)"
               class="com.objy.assist.data.actions.AddObjectActionDelegate"
               menubarPath="additions"
               enablesFor="1"
               id="com.objy.assist.data.action7">
